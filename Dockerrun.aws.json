{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name":"client",
            "image": "terriwunde/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "server",
            "image": "terriwunde/multi-server",
            "hostname": "api",
            "essential":false
        },
        {
            "name": "worker",
            "image:": "terriwunde/multi-worker",
            "hostname":"worker",
            "essential":false
        },
        {
            "name":"nginx",
            "image":"terriwunde/multi-nginx",
            "hostname":"nginx",
            "essential":true,
            "portMappings": [
                {
                    "hostPort":80,
                    "containerPort":80
                }
            ],
            "links":[
                "client","server"
            ]

        }
    ]
}